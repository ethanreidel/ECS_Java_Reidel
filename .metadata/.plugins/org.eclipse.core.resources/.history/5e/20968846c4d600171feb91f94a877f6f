
public class UsingVariables 
{
	public static void main(String[] args)
	{
		int room;
		double close;
		String learning;
		
		room = 113;
		close = 2.71828;
		learning = "Computer Science";
	
		System.out.println("This is room #");
		System.out.println("e is close to");
		System.out.println("I am learning a bit about");
	}
}