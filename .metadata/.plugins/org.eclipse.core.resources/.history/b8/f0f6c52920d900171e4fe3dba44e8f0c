
public class YourSchedule 
{
	public static void main (String[] args)
	{
		String Per1, Per2, Per3, Per4, Teacher1, Teacher2, Teacher3, Teacher4;
		
		Per1 = "Chemistry";
		Per2 = "ECS";
		Per3 = "US History";
		Per4 = "Free Period";
		Teacher1 =	"Dickinson";
		Teacher2 =	"Mauro";
		Teacher3 =	"Lockhart";
		Teacher4 =	"None";
		
		System.out.println("-----------------------------------");
		System.out.println("|Per 1	|"	+ Per1 + "   | "	 + Teacher1 +"   |");
		System.out.println("|Per 1	|"	+ Per2 + "         |        "	 + Teacher2 +"|");
		System.out.println("|Per 1	|"	+ Per3 + "  | "   + Teacher3 +"    |");
		System.out.println("|Per 1	|"	+ Per4 + " | "   + Teacher4 +"        |   ");		
		System.out.println("-----------------------------------");
	
		
}
}
